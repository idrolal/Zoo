

<h2>Список животных нашего зоопарка</h2>
<ul>
{{#each animals}}
   <div data-id="{{id}}">
      <img width="100" height="100" src="/img/{{photo}}">
      <p>{{name}}</p>
      <p>Описание: {{description}}</p>
      <button id='buttonDelete'  value= {{id}}>Удалить</button>
      <a href="/changeAnimal/edit" >Редактировать</a>
    </div> 
{{/each}}
</ul>
